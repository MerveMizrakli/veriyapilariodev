#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

struct Ogrenci {
	
	char isim [20];
	char soyisim [20];
	int numara;
	int yas;
	char bolum [30];
	char adres_bilgisi[200];
	
	
}ogrenci1,ogrenci2,ogrenci3,ogrenci4,ogrenci5;




int main(int argc, char *argv[]) {
	
	printf("Lutfen 1. ogrencinin bilgilerini giriniz:\n\n");
	
