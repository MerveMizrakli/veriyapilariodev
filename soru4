	scanf("%s",&ogrenci1.isim);
	
	printf("Soyisim: ");
	scanf("%s",&ogrenci1.soyisim);
	
	printf("Numara: ");
	scanf("%d",&ogrenci1.numara);
	
	printf("Yas: ");
	scanf("%d",&ogrenci1.yas);
	
	printf("Bolum: ");
	scanf("%s",&ogrenci1.bolum);
	
	printf("Adres bilgisi: ");
	scanf("%s",&ogrenci1.adres_bilgisi);
	
	
